@model List<SP_IP.Models.IpModel>

<h1>IP Data Results</h1>

<table>
    <thead>
        <tr>
            <th>Column 1</th>
            <th>Column 2</th>
            <th>Column 3</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CountryName</td>
                <td>@item.AddressesCount</td>
                <td>@item.LastAddressUpdated</td>
            </tr>
        }
    </tbody>
</table>
